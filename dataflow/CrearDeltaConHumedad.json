{
	"name": "CrearDeltaConHumedad",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_DT_temperatura",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "ADLS_WCALCAGNO",
						"type": "LinkedServiceReference"
					},
					"name": "Delta1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          time as timestamp,",
				"          latitud as double,",
				"          longitud as double,",
				"          Ts_Valor as double,",
				"          CodigoNacional as double,",
				"          nombreEstacion as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     compressionType: 'snappy',",
				"     compressionLevel: 'Fastest',",
				"     fileSystem: 'refined',",
				"     folderPath: 'DeltaParquet',",
				"     overwrite:true,",
				"     mergeSchema: true,",
				"     autoCompact: true,",
				"     optimizedWrite: true,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0222,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Delta1"
			]
		}
	}
}