{
	"name": "DeltaHumedadDinamico",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "ADLS_WCALCAGNO",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     periodo as string ('1951')",
				"}",
				"source(useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     fileList: true,",
				"     format: 'delimited',",
				"     fileSystem: 'landing',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true,",
				"     wildcardPaths:[(concat('Datos-CambioClimatico/main/output/humedad_dmc/',$periodo,'/',$periodo,'_humedad_dmc.csv'))],",
				"     partitionBy('hash', 2,",
				"          ERROR_FUNCTION('')",
				"     )) ~> source1"
			]
		}
	}
}